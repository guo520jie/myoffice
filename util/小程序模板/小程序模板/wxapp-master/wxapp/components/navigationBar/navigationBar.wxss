/* 小程序自定义头部bar */
.navigationBar{ width: 100%;  background-color: lightblue;}
.navigationBar .bar{ width: 100%; display: flex; align-items: center; padding: 0 2rem; background-color: lightpink;}
.navigationBar .text{ color: #fff; font-size: 0.32rem; display: block;}
.navigationBar .title{ flex: 1; text-align: center;}
.navigationBar .back{ position: absolute; left: 0.3rem; top: 50%; width: 1.5rem; height: 0.32rem; margin-top: -0.16rem; display: flex; align-items: center;}
.navigationBar .back .ic{ width:0.16rem; height: 0.3rem; margin-right: 0.1rem;   background-image:url('data:image/png;base64,');}